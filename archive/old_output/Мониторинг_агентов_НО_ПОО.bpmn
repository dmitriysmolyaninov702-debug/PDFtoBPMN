<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" 
                   xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
                   xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" 
                   xmlns:di="http://www.omg.org/spec/DD/20100524/DI" 
                   xmlns:camunda="http://camunda.org/schema/1.0/bpmn" 
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                   id="Definitions_Monitoring" 
                   targetNamespace="http://bpmn.io/schema/bpmn" 
                   exporter="Camunda Modeler" 
                   exporterVersion="5.0.0">
  
  <bpmn:collaboration id="Collaboration_Monitoring_Agents">
    <bpmn:participant id="Participant_Monitoring" name="Мониторинг агентов по наземному обслуживанию и ПОО ВС (ДП-М1.020-06)" processRef="Process_Monitoring_Agents" />
  </bpmn:collaboration>
  
  <bpmn:process id="Process_Monitoring_Agents" name="Процесс мониторинга агентов" isExecutable="true">
    
    <!-- Определение дорожек (Lanes) на основе RACI матрицы -->
    <bpmn:laneSet id="LaneSet_Monitoring">
      
      <bpmn:lane id="Lane_Head_DONO" name="Руководитель ДОНО">
        <bpmn:flowNodeRef>Task_ApproveProgram</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ReviewSAG</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_RiskLevel</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_InformManagement</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_ProcessComplete</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_Head_OOOD" name="Начальник ОООД">
        <bpmn:flowNodeRef>Task_CorrectProgram</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_InformTopManagement</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_EvaluateCA</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_Head_OONO" name="Начальник ООНО">
        <bpmn:flowNodeRef>Task_EnterNonconformities</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ControlDeadlines</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_CAImplemented</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_LeadManager_OOOD" name="Ведущий менеджер ОООД">
        <bpmn:flowNodeRef>StartEvent_StartMonitoring</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_DevelopProgram</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ControlExecution</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_PrepareAnnualReport</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_RiskEngineer" name="Инженер по анализу рисков ООНО">
        <bpmn:flowNodeRef>Task_CollectData</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ProcessAndAnalyze</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_RankAirports</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AssessRisks</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_ImmediateAction</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_Manager_OONO" name="Менеджер ООНО">
        <bpmn:flowNodeRef>Task_SystemicMonitoring</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_WorkWithAgents</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_WorkRequired</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SendRequestToAirport</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_MonitorCA</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_AuditHead" name="Руководитель аудиторской группы">
        <bpmn:flowNodeRef>Task_PrepareAudit</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ConductAudit</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_PrepareAuditReport</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_Auditor" name="Аудитор">
        <bpmn:flowNodeRef>Task_ExecuteAudit</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_DocumentAudit</bpmn:flowNodeRef>
      </bpmn:lane>
      
    </bpmn:laneSet>
    
    <!-- ЭТАП 1: ПЛАНИРОВАНИЕ И РАЗРАБОТКА ПРОГРАММЫ -->
    <bpmn:startEvent id="StartEvent_StartMonitoring" name="Начало процесса мониторинга">
      <bpmn:outgoing>Flow_Start_To_Develop</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:task id="Task_DevelopProgram" name="Разработка программы мониторинга агентов">
      <bpmn:incoming>Flow_Start_To_Develop</bpmn:incoming>
      <bpmn:outgoing>Flow_Develop_To_Approve</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_ApproveProgram" name="Утверждение программы мониторинга">
      <bpmn:incoming>Flow_Develop_To_Approve</bpmn:incoming>
      <bpmn:outgoing>Flow_Approve_To_Control</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_ControlExecution" name="Контроль выполнения программы">
      <bpmn:incoming>Flow_Approve_To_Control</bpmn:incoming>
      <bpmn:outgoing>Flow_Control_To_Audit</bpmn:outgoing>
    </bpmn:task>
    
    <!-- ЭТАП 2: ПРОВЕДЕНИЕ АУДИТА -->
    <bpmn:task id="Task_PrepareAudit" name="Подготовка и планирование аудита">
      <bpmn:incoming>Flow_Control_To_Audit</bpmn:incoming>
      <bpmn:outgoing>Flow_Prepare_To_Conduct</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_ConductAudit" name="Проведение аудита">
      <bpmn:incoming>Flow_Prepare_To_Conduct</bpmn:incoming>
      <bpmn:outgoing>Flow_Conduct_To_Execute</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_ExecuteAudit" name="Выполнение аудита по чек-листам">
      <bpmn:incoming>Flow_Conduct_To_Execute</bpmn:incoming>
      <bpmn:outgoing>Flow_Execute_To_Document</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_DocumentAudit" name="Документирование результатов аудита">
      <bpmn:incoming>Flow_Execute_To_Document</bpmn:incoming>
      <bpmn:outgoing>Flow_Document_To_Report</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_PrepareAuditReport" name="Подготовка отчета о проведенном аудите">
      <bpmn:incoming>Flow_Document_To_Report</bpmn:incoming>
      <bpmn:outgoing>Flow_Report_To_Collect</bpmn:outgoing>
    </bpmn:task>
    
    <!-- ЭТАП 3: СИСТЕМНЫЙ МОНИТОРИНГ И ОБРАБОТКА ДАННЫХ -->
    <bpmn:task id="Task_CollectData" name="Сбор данных о состоянии аэродромов">
      <bpmn:incoming>Flow_Report_To_Collect</bpmn:incoming>
      <bpmn:outgoing>Flow_Collect_To_Process</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_ProcessAndAnalyze" name="Обработка, оценка и анализ данных">
      <bpmn:incoming>Flow_Collect_To_Process</bpmn:incoming>
      <bpmn:outgoing>Flow_Process_To_Check</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:exclusiveGateway id="Gateway_WorkRequired" name="Требуется работа с аэропортом?">
      <bpmn:incoming>Flow_Process_To_Check</bpmn:incoming>
      <bpmn:outgoing>Flow_Work_Required_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_Work_Required_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:exclusiveGateway id="Gateway_ImmediateAction" name="Требуются незамедлительные действия?">
      <bpmn:incoming>Flow_Work_Required_Yes</bpmn:incoming>
      <bpmn:outgoing>Flow_Immediate_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_Immediate_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:task id="Task_RankAirports" name="Ранжирование аэропортов по факторам опасности">
      <bpmn:incoming>Flow_Immediate_No</bpmn:incoming>
      <bpmn:outgoing>Flow_Rank_To_Systemic</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_SystemicMonitoring" name="Проведение системного мониторинга">
      <bpmn:incoming>Flow_Rank_To_Systemic</bpmn:incoming>
      <bpmn:outgoing>Flow_Systemic_To_Send</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_SendRequestToAirport" name="Направление запроса в аэропорт для разработки КД">
      <bpmn:incoming>Flow_Systemic_To_Send</bpmn:incoming>
      <bpmn:outgoing>Flow_Send_To_Work</bpmn:outgoing>
    </bpmn:task>
    
    <!-- ЭТАП 4: РАБОТА С НЕСООТВЕТСТВИЯМИ -->
    <bpmn:task id="Task_WorkWithAgents" name="Проведение работы с контрагентом по направлению деятельности">
      <bpmn:incoming>Flow_Send_To_Work</bpmn:incoming>
      <bpmn:incoming>Flow_Immediate_Yes</bpmn:incoming>
      <bpmn:outgoing>Flow_Work_To_Control</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_ControlDeadlines" name="Контроль принятия КД, поступления ответа об устранении">
      <bpmn:incoming>Flow_Work_To_Control</bpmn:incoming>
      <bpmn:outgoing>Flow_Control_To_Check</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:exclusiveGateway id="Gateway_CAImplemented" name="КД приняты, несоответствие устранено?">
      <bpmn:incoming>Flow_Control_To_Check</bpmn:incoming>
      <bpmn:outgoing>Flow_CA_Implemented_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_CA_Implemented_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:task id="Task_EnterNonconformities" name="Внесение несоответствий категории 1F в ИС СУБП">
      <bpmn:incoming>Flow_CA_Implemented_No</bpmn:incoming>
      <bpmn:outgoing>Flow_Enter_To_SAG</bpmn:outgoing>
    </bpmn:task>
    
    <!-- ЭТАП 5: ЭСКАЛАЦИЯ И ПРИНЯТИЕ РЕШЕНИЙ -->
    <bpmn:task id="Task_ReviewSAG" name="Рассмотрение на SAG GRH ДОНО (ДУБП, ЛД при необходимости)">
      <bpmn:incoming>Flow_Enter_To_SAG</bpmn:incoming>
      <bpmn:outgoing>Flow_SAG_To_Assess</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_AssessRisks" name="Проведение совместной оценки рисков">
      <bpmn:incoming>Flow_SAG_To_Assess</bpmn:incoming>
      <bpmn:outgoing>Flow_Assess_To_Risk</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:exclusiveGateway id="Gateway_RiskLevel" name="Риски допустимые?">
      <bpmn:incoming>Flow_Assess_To_Risk</bpmn:incoming>
      <bpmn:outgoing>Flow_Risk_Acceptable</bpmn:outgoing>
      <bpmn:outgoing>Flow_Risk_Unacceptable</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:task id="Task_MonitorCA" name="Мониторинг/анализ принимаемых КД аэропортом">
      <bpmn:incoming>Flow_Risk_Acceptable</bpmn:incoming>
      <bpmn:incoming>Flow_CA_Implemented_Yes</bpmn:incoming>
      <bpmn:outgoing>Flow_Monitor_To_Evaluate</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_InformManagement" name="Информирование зам.ГД для решения о прекращении/возобновлении полетов">
      <bpmn:incoming>Flow_Risk_Unacceptable</bpmn:incoming>
      <bpmn:outgoing>Flow_Inform_To_Continue</bpmn:outgoing>
    </bpmn:task>
    
    <!-- ЭТАП 6: ИНФОРМИРОВАНИЕ И ОТЧЕТНОСТЬ -->
    <bpmn:task id="Task_EvaluateCA" name="Оценка достаточности выполненных КД">
      <bpmn:incoming>Flow_Monitor_To_Evaluate</bpmn:incoming>
      <bpmn:outgoing>Flow_Evaluate_To_Inform</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_InformTopManagement" name="Информирование высшего руководства по итогам">
      <bpmn:incoming>Flow_Evaluate_To_Inform</bpmn:incoming>
      <bpmn:incoming>Flow_Inform_To_Continue</bpmn:incoming>
      <bpmn:outgoing>Flow_Inform_To_Report</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_PrepareAnnualReport" name="Подготовка годового отчета по программе">
      <bpmn:incoming>Flow_Inform_To_Report</bpmn:incoming>
      <bpmn:incoming>Flow_Work_Required_No</bpmn:incoming>
      <bpmn:outgoing>Flow_Report_To_Correct</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_CorrectProgram" name="Корректирование программы мониторинга">
      <bpmn:incoming>Flow_Report_To_Correct</bpmn:incoming>
      <bpmn:outgoing>Flow_Correct_To_End</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:endEvent id="EndEvent_ProcessComplete" name="Процесс завершен (цикл мониторинга)">
      <bpmn:incoming>Flow_Correct_To_End</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Определение потоков последовательности (Sequence Flows) -->
    <bpmn:sequenceFlow id="Flow_Start_To_Develop" sourceRef="StartEvent_StartMonitoring" targetRef="Task_DevelopProgram" />
    <bpmn:sequenceFlow id="Flow_Develop_To_Approve" sourceRef="Task_DevelopProgram" targetRef="Task_ApproveProgram" />
    <bpmn:sequenceFlow id="Flow_Approve_To_Control" sourceRef="Task_ApproveProgram" targetRef="Task_ControlExecution" />
    <bpmn:sequenceFlow id="Flow_Control_To_Audit" sourceRef="Task_ControlExecution" targetRef="Task_PrepareAudit" />
    <bpmn:sequenceFlow id="Flow_Prepare_To_Conduct" sourceRef="Task_PrepareAudit" targetRef="Task_ConductAudit" />
    <bpmn:sequenceFlow id="Flow_Conduct_To_Execute" sourceRef="Task_ConductAudit" targetRef="Task_ExecuteAudit" />
    <bpmn:sequenceFlow id="Flow_Execute_To_Document" sourceRef="Task_ExecuteAudit" targetRef="Task_DocumentAudit" />
    <bpmn:sequenceFlow id="Flow_Document_To_Report" sourceRef="Task_DocumentAudit" targetRef="Task_PrepareAuditReport" />
    <bpmn:sequenceFlow id="Flow_Report_To_Collect" sourceRef="Task_PrepareAuditReport" targetRef="Task_CollectData" />
    <bpmn:sequenceFlow id="Flow_Collect_To_Process" sourceRef="Task_CollectData" targetRef="Task_ProcessAndAnalyze" />
    <bpmn:sequenceFlow id="Flow_Process_To_Check" sourceRef="Task_ProcessAndAnalyze" targetRef="Gateway_WorkRequired" />
    <bpmn:sequenceFlow id="Flow_Work_Required_Yes" name="Да" sourceRef="Gateway_WorkRequired" targetRef="Gateway_ImmediateAction">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${workRequired == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Work_Required_No" name="Нет" sourceRef="Gateway_WorkRequired" targetRef="Task_PrepareAnnualReport" />
    <bpmn:sequenceFlow id="Flow_Immediate_Yes" name="Да" sourceRef="Gateway_ImmediateAction" targetRef="Task_WorkWithAgents">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${immediateAction == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Immediate_No" name="Нет" sourceRef="Gateway_ImmediateAction" targetRef="Task_RankAirports" />
    <bpmn:sequenceFlow id="Flow_Rank_To_Systemic" sourceRef="Task_RankAirports" targetRef="Task_SystemicMonitoring" />
    <bpmn:sequenceFlow id="Flow_Systemic_To_Send" sourceRef="Task_SystemicMonitoring" targetRef="Task_SendRequestToAirport" />
    <bpmn:sequenceFlow id="Flow_Send_To_Work" sourceRef="Task_SendRequestToAirport" targetRef="Task_WorkWithAgents" />
    <bpmn:sequenceFlow id="Flow_Work_To_Control" sourceRef="Task_WorkWithAgents" targetRef="Task_ControlDeadlines" />
    <bpmn:sequenceFlow id="Flow_Control_To_Check" sourceRef="Task_ControlDeadlines" targetRef="Gateway_CAImplemented" />
    <bpmn:sequenceFlow id="Flow_CA_Implemented_Yes" name="Да" sourceRef="Gateway_CAImplemented" targetRef="Task_MonitorCA" />
    <bpmn:sequenceFlow id="Flow_CA_Implemented_No" name="Нет" sourceRef="Gateway_CAImplemented" targetRef="Task_EnterNonconformities" />
    <bpmn:sequenceFlow id="Flow_Enter_To_SAG" sourceRef="Task_EnterNonconformities" targetRef="Task_ReviewSAG" />
    <bpmn:sequenceFlow id="Flow_SAG_To_Assess" sourceRef="Task_ReviewSAG" targetRef="Task_AssessRisks" />
    <bpmn:sequenceFlow id="Flow_Assess_To_Risk" sourceRef="Task_AssessRisks" targetRef="Gateway_RiskLevel" />
    <bpmn:sequenceFlow id="Flow_Risk_Acceptable" name="Да" sourceRef="Gateway_RiskLevel" targetRef="Task_MonitorCA" />
    <bpmn:sequenceFlow id="Flow_Risk_Unacceptable" name="Нет" sourceRef="Gateway_RiskLevel" targetRef="Task_InformManagement">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${riskUnacceptable == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Monitor_To_Evaluate" sourceRef="Task_MonitorCA" targetRef="Task_EvaluateCA" />
    <bpmn:sequenceFlow id="Flow_Inform_To_Continue" sourceRef="Task_InformManagement" targetRef="Task_InformTopManagement" />
    <bpmn:sequenceFlow id="Flow_Evaluate_To_Inform" sourceRef="Task_EvaluateCA" targetRef="Task_InformTopManagement" />
    <bpmn:sequenceFlow id="Flow_Inform_To_Report" sourceRef="Task_InformTopManagement" targetRef="Task_PrepareAnnualReport" />
    <bpmn:sequenceFlow id="Flow_Report_To_Correct" sourceRef="Task_PrepareAnnualReport" targetRef="Task_CorrectProgram" />
    <bpmn:sequenceFlow id="Flow_Correct_To_End" sourceRef="Task_CorrectProgram" targetRef="EndEvent_ProcessComplete" />
    
  </bpmn:process>
  
  <!-- Диаграмма BPMN (визуальное представление) -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_Monitoring">
    <bpmndi:BPMNPlane id="BPMNPlane_Monitoring" bpmnElement="Collaboration_Monitoring_Agents">
      
      <!-- Participant (Pool) -->
      <bpmndi:BPMNShape id="Participant_Monitoring_di" bpmnElement="Participant_Monitoring" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="4800" height="1600" />
      </bpmndi:BPMNShape>
      
      <!-- Lanes -->
      <bpmndi:BPMNShape id="Lane_Head_DONO_di" bpmnElement="Lane_Head_DONO" isHorizontal="true">
        <dc:Bounds x="190" y="80" width="4770" height="200" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_Head_OOOD_di" bpmnElement="Lane_Head_OOOD" isHorizontal="true">
        <dc:Bounds x="190" y="280" width="4770" height="200" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_Head_OONO_di" bpmnElement="Lane_Head_OONO" isHorizontal="true">
        <dc:Bounds x="190" y="480" width="4770" height="200" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_LeadManager_OOOD_di" bpmnElement="Lane_LeadManager_OOOD" isHorizontal="true">
        <dc:Bounds x="190" y="680" width="4770" height="200" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_RiskEngineer_di" bpmnElement="Lane_RiskEngineer" isHorizontal="true">
        <dc:Bounds x="190" y="880" width="4770" height="200" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_Manager_OONO_di" bpmnElement="Lane_Manager_OONO" isHorizontal="true">
        <dc:Bounds x="190" y="1080" width="4770" height="200" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_AuditHead_di" bpmnElement="Lane_AuditHead" isHorizontal="true">
        <dc:Bounds x="190" y="1280" width="4770" height="200" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_Auditor_di" bpmnElement="Lane_Auditor" isHorizontal="true">
        <dc:Bounds x="190" y="1480" width="4770" height="200" />
      </bpmndi:BPMNShape>
      
      <!-- Элементы процесса в дорожке Ведущий менеджер ОООД -->
      <bpmndi:BPMNShape id="StartEvent_StartMonitoring_di" bpmnElement="StartEvent_StartMonitoring">
        <dc:Bounds x="252" y="762" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="227" y="805" width="86" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_DevelopProgram_di" bpmnElement="Task_DevelopProgram">
        <dc:Bounds x="340" y="740" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_ControlExecution_di" bpmnElement="Task_ControlExecution">
        <dc:Bounds x="610" y="740" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_PrepareAnnualReport_di" bpmnElement="Task_PrepareAnnualReport">
        <dc:Bounds x="4440" y="740" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Элементы в дорожке Руководитель ДОНО -->
      <bpmndi:BPMNShape id="Task_ApproveProgram_di" bpmnElement="Task_ApproveProgram">
        <dc:Bounds x="475" y="140" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_ReviewSAG_di" bpmnElement="Task_ReviewSAG">
        <dc:Bounds x="3180" y="140" width="100" height="80" />
